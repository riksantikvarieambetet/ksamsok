<?xml version="1.0" encoding="UTF-8"?>
<rdf:RDF 
	xmlns="http://kulturarvsdata.se/ksamsok#" 
	xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
	xml:base="http://kulturarvsdata.se/resurser/EntityType">
	
	<EntityType rdf:ID="object">
		<name>Objekt/föremål</name>
		<description>Information om t.ex. museiföremål. Underkategori till Fysiskt ting.</description>
	</EntityType>
	
	<EntityType rdf:ID="photo">
		<name>Foto</name>
		<description>Information om fotografier. Underkategori till Fysiskt ting.</description>
	</EntityType>

	<EntityType rdf:ID="interactiveResource">
		<name>Bearbetning/interaktiv resurs</name>
		<description>Information om en interaktiv resurs som texter, Internet-sidor och multimedia. Underkategori till Fysiskt ting.</description>
	</EntityType>

	<EntityType rdf:ID="video">
		<name>Film/video</name>
		<description>Information om rörlig bild. Underkategori till Fysiskt ting.</description>
	</EntityType>

	<EntityType rdf:ID="sound">
		<name>Ljud</name>
		<description>Information om ljudupptagning/inspelning. Underkategori till Fysiskt ting.</description>
	</EntityType>

	<EntityType rdf:ID="document">
		<name>Dokument</name>
		<description>Information om ett dokument eller arkivhandling som finns tillgänglig i fulltextformat. Underkategori till Fysiskt ting.</description>
	</EntityType>

	<EntityType rdf:ID="book">
		<name>Bok</name>
		<description>Information om en publikation som finns tillgänglig i fulltextformat. Underkategori till Fysiskt ting.</description>
	</EntityType>

	<EntityType rdf:ID="drawing">
		<name>Teckning</name>
		<description>Information om en teckning. Underkategori till Fysiskt ting.</description>
	</EntityType>

	<EntityType rdf:ID="art">
		<name>Konstverk</name>
		<description>Information om ett konstverk. Underkategori till Fysiskt ting.</description>
	</EntityType>

	<EntityType rdf:ID="blueprint">
		<name>Ritning</name>
		<description>Information om en ritning t.ex. en arkitektritning. Underkategori till Fysiskt ting.</description>
	</EntityType>
	
	<EntityType rdf:ID="map">
		<name>Karta</name>
		<description>Information om en karta. Underkategori till Fysiskt ting.</description>
	</EntityType>

	<EntityType rdf:ID="collection">
		<name>Samling</name>
		<description>Information om en samling eller annat urval. Underkategori till Fysiskt ting.</description>
	</EntityType>

	<EntityType rdf:ID="site">
		<name>Miljö</name>
		<description>Ersätts av Kulturmiljö men finns kvar för bakåtkompatibilitet. Underkategori till Fysiskt ting.</description>
	</EntityType>
	
	<EntityType rdf:ID="culturallandscape">
		<name>Kulturmiljö</name>
		<description>Information om en större miljö som t.ex. innefattar fornlämningar och/eller bebyggelse (exempelvis en kyrkomiljö eller ett riksintresse). Underkategori till Fysiskt ting.</description>
	</EntityType>
	
	<EntityType rdf:ID="monument">
		<name>Kulturlämning</name>
		<description>Information om en kulturlämning i kulturlandskapet t.ex. en husgrund eller ett fornminne. Underkategori till Fysiskt ting.</description>
	</EntityType>
	
	<EntityType rdf:ID="building">
		<name>Byggnad</name>
		<description>Information om en byggnad. Underkategori till Fysiskt ting.</description>
	</EntityType>

	<EntityType rdf:ID="person">
		<name>Person</name>
		<description>Information om en person. Underkategori till Agent.</description>
	</EntityType>

	<EntityType rdf:ID="organization">
		<name>Organisation</name>
		<description>Information om en organisation. Underkategori till Agent.</description>
	</EntityType>

	<EntityType rdf:ID="group">
		<name>Grupp</name>
		<description>Information om en grupp personer (t.ex. konstnärsgrupp). Underkategori till Agent.</description>
	</EntityType>

	<EntityType rdf:ID="event">
		<name>Historisk händelse</name>
		<description>Information om en händelse, historisk eller nutida. Underkategori till Event.</description>
	</EntityType>

	<EntityType rdf:ID="display">
		<name>Utställning</name>
		<description>Information om en fysisk eller digital utställning. Underkategori till Event.</description>
	</EntityType>

	<EntityType rdf:ID="concept">
		<name>Koncept</name>
		<description>En övergripande definition av ett event eller ett objekt t.ex. av krig.</description>
	</EntityType>

	<EntityType rdf:ID="story">
		<name>Berättelse</name>
		<description>En berättelse om t ex ett föremål eller fristående. Underkategori till Informationsobjekt.</description>
	</EntityType>

	<EntityType rdf:ID="3d-model">
		<name>3D-modell</name>
		<description>Information om en digital 3D-modell. Underkategori till Informationsobjekt.</description>
	</EntityType>

</rdf:RDF>
